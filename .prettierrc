{
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ],
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-tailwindcss"
    ],
    "printWidth": 120,
    "singleQuote": true,
    "tailwindStylesheet": "./src/app.css",
    "trailingComma": "es5",
    "useTabs": false
}
