{
    "name": "gaming-cafe-booker",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "packageManager": "pnpm@10.17.1",
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "pnpm run build && wrangler d1 migrations apply gaming-booker && wrangler dev",
        "prepare": "svelte-kit sync || echo ''",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:migrate:staging": "wrangler d1 migrations apply gaming-booker-dev --remote",
        "db:migrate:prod": "wrangler d1 migrations apply gaming-booker --remote",
        "db:studio": "drizzle-kit studio",
        "cf:deploy": "pnpm run build && wrangler deploy",
        "cf:typegen": "wrangler types ./src/worker-configuration.d.ts"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20251014.0",
        "@eslint/compat": "^1.4.1",
        "@eslint/js": "^9.38.0",
        "@iconify-json/mdi": "^1.2.3",
        "@iconify-json/ri": "^1.2.6",
        "@iconify-json/simple-icons": "^1.2.56",
        "@libsql/client": "^0.15.15",
        "@sveltejs/adapter-cloudflare": "^7.2.4",
        "@sveltejs/kit": "^2.48.3",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.1.16",
        "@types/luxon": "^3.7.1",
        "@types/node": "^24.9.2",
        "bits-ui": "^2.14.1",
        "daisyui": "^5.3.10",
        "drizzle-kit": "^0.31.6",
        "drizzle-zod": "^0.8.3",
        "eslint": "^9.38.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-svelte": "^3.12.5",
        "globals": "^16.4.0",
        "luxon": "^3.7.2",
        "nanoid": "^5.1.6",
        "prettier": "^3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "runed": "^0.35.1",
        "svelte": "^5.43.0",
        "svelte-check": "^4.3.3",
        "svelte-html-modal": "^3.2.0",
        "svelte-modals": "^2.0.1",
        "svelte-sonner": "^1.0.5",
        "svelte-trix": "^0.0.6",
        "tailwindcss": "^4.1.16",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.2",
        "unplugin-icons": "^22.5.0",
        "vite": "^7.1.12",
        "vite-plugin-devtools-json": "^1.0.0",
        "vite-plugin-mkcert": "^1.17.9",
        "zod": "^4.1.12"
    },
    "dependencies": {
        "@fullcalendar/core": "^6.1.19",
        "@fullcalendar/daygrid": "^6.1.19",
        "@fullcalendar/interaction": "^6.1.19",
        "@fullcalendar/list": "^6.1.19",
        "@fullcalendar/timegrid": "^6.1.19",
        "better-auth": "^1.3.34",
        "drizzle-orm": "^0.44.7",
        "remove": "^0.1.5"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    }
}
